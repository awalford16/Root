{
  "expo": {
    "name": "Blank Template",
    "slug": "GreenPath",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uol.greenpath",
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "package": "com.uol.greenpath",
      "config": {
        "googleMaps": {"apiKey": "AIzaSyCp3_KwYYF7mFU_mqRtjQB9a1-Df7L4piI"}
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
